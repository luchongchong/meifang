@charset "utf-8";
/* CSS Document */
body,p,h4,ul,li,h1,dl,h2,h3,input,form{margin:0;padding:0}
li{list-style:none}
a{text-decoration:none;color:#939292}
img{border:0}
/*重置*/
#top{background-color:#fee5e1;height:90px}
#header{width:1000px;margin:0 auto;background:url(../images/top.png) no-repeat left;height:90px}
h1.logo{margin-left:10px;width:170px;float:left;display:inline}
h1.topR{width:244px;float:right;margin-right:10px;margin-top:13px}
h1.topR a{font-size:14px; margin-left:10px}
.topR a.log{ margin-left:170px}
form.sear{position:relative}
input.text{width:228px;height:27px;border:3px #f00 solid;border-radius:4px;margin-top:8px;padding-left:10px;
color:#8e8b8b;font-size:15px;font-family:"微软雅黑"}
input.search{position:absolute;top:15px;left:210px}
#nav{background-color:#D90156;height:40px}
#list{margin:0 auto;width:1000px;height:40px;line-height:40px}
#list li{float:left}
#list li a{font-family:"微软雅黑";float:left;width:125px;text-align:center;line-height:40px;font-size:18px;color:#fff}
#list li a:hover,#list li a.select{background-color:#ad0246}
/*header*/
#listing{background-color:#fff;height:180px;padding-top:20px}
#entry{width:960px;margin:0 auto;height:160px; padding-left:40px}
ul.after{float:left;margin-right:50px;font-size:13px;width:100px}
ul.after a{color:#5B6166;float:left;line-height:23px}
ul.after a.bold{font-weight:bold;font-size:15px}
ul.after a:hover{text-decoration:underline}
img.ico{float:left;width:33px;margin-right:6px}
#footer{background-color:#d90156;height:110px}
#trail{width:1000px;margin:0 auto;font-size:13px;padding-top:28px}
p.tabul{color:#fff;text-align:center;line-height:30px}
p.tabul a{color:#fff}
p.tabul a:hover{text-decoration:underline}
/*footer*/
#box{background-color:#eee;width:100%;padding-top:30px;padding-bottom:30px;font-family:"微软雅黑"}
#content{width:1000px;margin:0 auto;background-color:#fff;padding-left:55px;padding-top:15px;overflow:hidden}
#content p.bread{font-size:13px;color:#D90055;padding-bottom:10px}
#content p.bread a{color:#D90055}
p.imgSmall{margin-top:10px;clear:both}
p.imgSmall img{margin-right:10px;border:#Fff 2px solid}
p.imgSmall img.imgBor,.title li.limg img.imgBor,.title li.limg span.imgBor{border:#F00 2px solid}
p.share span{padding-left:30px}
p.share a{color:#000}
p.share{margin-top:10px}
p.share img.img1{padding-bottom:5px}
.imggs{float:left;margin-right:40px}
.title{margin-top:20px;float:left;padding-bottom:20px}
.title h4{margin-bottom:20px}
.title p{font-size:14px;color:#646464;margin-bottom:12px}
.title p span,.title dl span{color:#999;float:left;width:60px}
.title p b{font-size:18px;color:#D20221}
.title p strong{padding-left:7px;padding-right:7px;background-color:#EBEBEB}
.title li{font-size:12px;line-height:25px;color:#D00}
.title li.man,.title dl.man{margin-left:60px}
.title li a,.title dl.man a{color:#C00}
.title li a.kong{padding-left:20px}
.title li a.xiang,.title li span{color:#333}
.title li a.xiang{padding-left:20px}
.title p a.ju{padding-left:15px}
.title select.play{padding:3px;background-color:#E8E8E8;color:#333;border:0}
.title p a.free{color:#03F;font-size:14px}
.title dl span{color:#333;font-size:14px}
.title li.limg{margin-top:13px}
.title li.limg img{cursor:pointer;border:#C7C7C7 2px solid;padding:2px;margin-right:5px}
.title li.limg span{cursor:pointer;border:#D2D2D2 2px solid;text-align:center;margin-right:7px}
.title dl.amount{margin-top:60px}
.title dl.amount li{cursor:pointer;width:102px;height:26px;border:#E5E5E5 1px solid;text-align:center;color:#333;float:left;margin-right:10px}
.title dl.amount input{cursor:pointer;width:30px;height:30px;border:0}
.title dl.amount input.payText{width:50px;text-align:center}
.title dl.amount b{font-size:14px}
.title dl.man{margin-top:20px;font-size:14px}
.title li.man a.sub{ float:left;cursor:pointer;margin-top:10px; margin-right:10px;width:130px;height:35px;line-height:35px;border:#F0CAB7 1px solid;background-color:#FFE4D1;color:#D90156;font-size:16px;font-family:"微软雅黑";text-align:center}
.title li.man a.but{float:left;cursor:pointer;margin-top:10px;width:140px;height:35px;line-height:35px;background-color:#D90156;color:#fff;font-size:16px;border:0;font-family:"微软雅黑"; text-align:center}

/*qiehuan*/
#qiehuan{width:1055px;margin:0 auto;background-color:#fff}
#inList{clear:both;border:#DBDBDB 1px solid;width:379px;height:33px;line-height:33px;margin-left:63px}
#inList li{overflow:hidden;float:left;cursor:pointer;font-size:16px;font-weight:bold;color:#666}
#inList li span{padding-left:13px;padding-right:13px;border-right:#DBDBDB 1px solid;float:left}
#inList li span.none{border:0}
#inCont{border-top:#EAEAEA 3px solid;width:930px;margin:0 auto}
.uls{margin-top:20px;font-size:14px;color:#333}
.uls ul{float:left;width:33%;line-height:26px}
.uls ul li span{color:#666}
.proDe1s{width:810px;clear:both;margin:0 auto;padding-top:30px}
.proDe1s img{padding-bottom:30px}
#inList li.inse{border-top:#F00 2px solid;background:url(images/inse.jpg) repeat-x}
/*放大镜*/
.jqzoom{border:1px solid #BBB;float:left;position:relative;padding:0px;cursor:pointer;}
div.zoomdiv {z-index:999;position:absolute;top:0px;left:0px;width:200px;height:200px;background:#fff;border:1px solid #CCC;display:none;text-align: center;overflow: hidden;}
div.jqZoomPup {z-index:99;visibility:hidden;position:absolute;top:0px;left:0px;width:50px;height:50px;border: 1px solid #aaa;background: #fff url(img/zoomlens.gif) 50% top  no-repeat;opacity: 0.5;-moz-opacity: 0.5;-khtml-opacity: 0.5;filter: alpha(Opacity=50);}
/*prodetails*/
#payCon{width:910px;margin:0 auto;overflow:hidden}
#payTop{background-color:#fff;width:100%;height:50px;border-bottom:#D8D8D8 2px solid}
#payMi{width:910px;margin:0 auto;line-height:50px}
.payToLe{float:left}
.payToLe img.zhi{float:left;margin-top:10px;padding-right:10px}
.payToLe a{font-size:20px;color:#000;float:left;padding-right:10px}
.payToLe select.select{border:0;font-size:14px;line-height:20px;margin-top:10px;background-color:#EFF0F2;float:left;padding:5px}
.payToRi{float:right;font-size:14px}
.payToRi span{padding-left:20px}
.payLogo{clear:both;font-size:24px}
.payLogo img{float:left;margin-right:10px}
.payLogo h2{color:#777576;padding-top:10px}
.ps{clear:both;padding-top:20px;overflow:hidden;zoom:1}
.ps p.left{float:left}
.ps p.left img{float:left}
.ps p.left span{float:left;margin-top:30px;margin-left:20px;font-size:14px}
.ps p.right{float:right;margin-top:30px}
.ps p.right span,.pN p.right span{font-size:23px;color:#DA0257;font-weight:bold}
#payNew{font-size:14px;overflow:hidden;zoom:1;clear:both;border-top:#B3B3B3 2px solid;border-bottom:#B3B3B3 2px solid;background-color:#fff}
#payCon p.order{clear:both;font-size:13px;color:#fff;float:right;background-color:#B3B3B3;width:64px;line-height:22px;text-align:center;
margin-right:30px}
.pN{width:820px;height:50px;border:#85A1D3 4px solid;margin:0 auto;margin-top:25px;margin-bottom:15px;line-height:50px;padding-left:20px;padding-right:20px}
.pN p.left{float:left}
.pN p.left img{padding-left:10px;padding-right:10px}
.pN p.left span{padding-left:20px;padding-right:15px}
.pN p.right{float:right}
.pN p.right span{font-size:18px}
.payWay,.payT,.payPass,input.subm{margin-left:21px}
.payWay p.solid,.payWay p.dashed{line-height:32px;color:#00AAF0;float:left;width:134px;text-align:center;border:#ccc 1px solid}
.payWay p.dashed{border:#ccc 1px dashed;margin-left:20px}
.payT{clear:both;padding-top:26px;padding-bottom:20px}
.payPass{font-size:14px;}
.payPass input{width:150px;height:23px;margin-top:10px;margin-right:10px}
.payPass a,.payWay a{color:#00AAEF}
.payPass p.six{color:#CCC;margin-top:5px}
input.subm{cursor:pointer;background-color:#01AAEF;border-radius:3px;border:0;width:99px;line-height:32px;color:#fff;margin-top:15px;margin-bottom:25px}
/*pay*/



/*返回顶部*/
	.backToTop{display:none;width:28px;line-height:1.2;padding:5px 0;background-color:#000;color:#fff;font-size:15px;
	text-align:center;position:fixed;_position:absolute;right:40px;bottom:110px;_bottom:"auto";cursor:pointer;opacity:.6;
	filter: Alpha(opacity=60)}